{
  "extends": [
    "dcl-tslint-config-standard",
    "tslint-react",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "no-commented-out-code": false,
    "prettier": [
      true,
      { "printWidth": 80, "singleQuote": true, "semi": false }
    ],
    "quotemark": [true, "single", "jsx-double"]
  },
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts", "./**/*.js"]
  }
}
